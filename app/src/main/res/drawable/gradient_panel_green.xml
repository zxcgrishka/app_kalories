<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#E3F3FC" />
        </shape>
    </item>
    
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="18dp" />
            <gradient
                android:startColor="#B2DBC1"
                android:endColor="#86D1D1"
                android:type="linear"
                android:angle="0" />
        </shape>
    </item>
</layer-list>